    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prember": "^1.0.5",
    "prettier": "^2.2.1",
    "qunit": "^2.15.0",
    "qunit-dom": "^1.6.0"
